<div class="container"><br/>
<%= link_to "New Conference", new_consortium_path, class: "btn btn-outline-info mr-sm-2" , role: "button" %><br/><br/>
<% @consortium.each do |article| %>
  <h3><%= article.name.html_safe %></h3>
  <p><%= article.date.html_safe %></p>
  <%= link_to "Edit Conference", edit_consortium_path(article) %>
  <br/>
  <%= link_to "Delete Conference", article, :confirm => "Are you sure?", :method => :delete  %>
  <% if article != @consortium.last %>
  <hr />
  <% end %>
<% end %>
</div>